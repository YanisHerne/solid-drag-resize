{
  "name": "solid-drag-resize",
  "version": "0.0.0",
  "description": "Solid-js library for creating draggable and resizable components.",
  "license": "MIT",
  "author": "Yanis Herne",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YanisHerne/solid-drag-resize.git"
  },
  "homepage": "https://github.com/YanisHerne/solid-drag-resize#readme",
  "bugs": {
    "url": "https://github.com/YanisHerne/solid-drag-resize/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": "./dist/index.jsx",
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve examples",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:ssr": "vitest --mode ssr",
    "test:browser": "vitest --config=vitest.browser.config.ts --mode headless --browser.viewport={1920,1080}",
    "browser": "vitest --config=vitest.browser.config.ts",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"examples/**/*.{js,ts,json,css,tsx,jsx}\"",
    "format:dry": "prettier --ignore-path .gitignore -c \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"examples/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-solid": "^0.14.5",
    "playwright": "^1.54.1",
    "prettier": "3.5.3",
    "solid-js": "^1.9.7",
    "tsup": "^8.5.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.7",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "solid",
    "drag",
    "resize",
    "components"
  ],
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.0"
  }
}
